{
  "manifest_version":2,
  "version":"1.0",
  "name":"ELIT formatter", 
  "description":"Formats ELIT rulings to be readable via a toggle button. Report issues @gongahkia on Github",
  "icons": {
    "16": "../asset/thinking-16.png",
    "24": "../asset/thinking-24.png",
    "32": "../asset/thinking-32.png",
    "64": "../asset/thinking-64.png",
    "128": "../asset/thinking-128.png"
  },
  "web_accessible_resources": [
    "../asset/thinking-16.png",
    "../asset/thinking-24.png",
    "../asset/thinking-32.png",
    "../asset/thinking-64.png",
    "../asset/thinking-128.png"
  ],
  "browser_action": {
    "default_icon": {
      "16": "../asset/thinking-16.png",
      "24": "../asset/thinking-24.png",
      "32": "../asset/thinking-32.png",
      "64": "../asset/thinking-64.png",
      "128": "../asset/thinking-128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
     "matches":[
        "https://www.elitigation.sg/gd/*",
        "https://www.elitigation.sg/gdviewer/*"
     ],
     "js":[
         "main.js"
     ]
    }
  ],
  "background": {
    "scripts": [
    ]
  },
  "permissions": [
    "activeTab"
  ]
}
